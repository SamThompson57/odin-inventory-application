<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <h1><%= title %></h1>
        <ul>
            <% if (locals.characters){%>
                <% characters.forEach(function(character){ %>
                <li><%=character.charactername%> - STRENGTH: <%=character.str%></li>
                <%}); %>
            <% } %>        
        </ul>
    </body>
</html>